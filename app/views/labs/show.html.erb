<div class="container labs">
  <br>
  <%= link_to("返回", labs_path, class: "btn btn-warning")%>
  <br>
  <br>
  <h1 class="h2 mb-3"><%= @lab.name %></h1>
  <br>
  <div class="table-responsive">
    <table class="table">
      <tbody>
        <tr>
          <td>id</td>
          <td><%= @lab.id %></td>
        </tr>
        <tr>
          <td>Name</td>
          <td><%= @lab.name %></td>
        </tr>
        <tr>
          <td>Position</td>
          <td><%= @lab.position %></td>
        </tr>
        <tr>
          <td>Website</td>
          <td><%= link_to @lab.website, @lab.website %></td>
        </tr>
        <tr>
          <td>Professors</td>
          <td>
            <% @lab.professors.each do |p| %>
              <%= link_to p.name, professor_path(p) %>,
            <% end %>
          </td>
        </tr>
        <tr>
          <td>Created at</td>
          <td><%= @lab.created_at.in_time_zone("Taipei") %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
